ralt - a : yabai -m window --focus west
ralt - s : yabai -m window --focus south
ralt - d : yabai -m window --focus north
ralt - f : yabai -m window --focus east

ralt - n : yabai -m window --focus next || yabai -m window --focus first
ralt - space : yabai -m window --toggle zoom-fullscreen
ralt - w : yabai -m window --toggle float

ralt + shift - a : yabai -m window --swap west
ralt + shift - s : yabai -m window --swap south
ralt + shift - d : yabai -m window --swap north
ralt + shift - f : yabai -m <M-D-w>window --swap east

ralt - z : yabai -m window west --resize right:-20:0 2> /dev/null || yabai -m window --resize right:-20:0
ralt - x : yabai -m window north --resize bottom:0:20 2> /dev/null || yabai -m window --resize bottom:0:20
ralt - c : yabai -m window south --resize top:0:-20 2> /dev/null || yabai -m window --resize top:0:-20
ralt - v : yabai -m window east --resize left:20:0 2> /dev/null || yabai -m window --resize left:20:0
